rules_version = '2';service cloud.firestore {match /databases/{database}/documents {// Allow public read access for the classes schedule
// but restrict writing to authenticated users (admin).
match /classes/{classId} {
  allow read: if true;
  allow write: if request.auth != null;
}

// Allow only authenticated users (admin) to read and write
// to the students collection.
match /students/{studentId} {
  allow read, write: if request.auth != null;
}

// Allow only authenticated users (admin) to read and write
// to the consultations collection.
match /consultations/{consultationId} {
    allow read, write: if request.auth != null;
}
}}